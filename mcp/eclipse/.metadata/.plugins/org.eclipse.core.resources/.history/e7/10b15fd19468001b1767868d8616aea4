package CakeClient.modules.combat;

import CakeClient.modules.Module;

public class AutoWtap extends Module
{
    private Float defaultGamma;
    
    public AutoWtap() {
        super("AutoWtap");
    }
    
    @Override
    public void onEnable() {
       
    }
    
    @Override
    public void onDisable() {
        
    }
    @Override
    public void onUpdate() {
        if (mc.thePlayer.attackEntityAsMob(null))
        	this.mc.gameSettings.gammaSetting = 72000.0f;
    }
    
}


